/*
 * generated by Xtext
 */
grammar DebugInternalLazyLinkingTestLanguage ;

// Rule Model
ruleModel :
	ruleType*
;

// Rule Type
ruleType :
	'type' RULE_ID (
		'extends' RULE_ID '.' RULE_ID
	)? (
		'for' RULE_ID 'in' RULE_ID
	)? '{' ruleProperty* ruleUnresolvedProxyProperty* '}'
;

// Rule Property
ruleProperty :
	RULE_ID+ RULE_ID ';'
;

// Rule UnresolvedProxyProperty
ruleUnresolvedProxyProperty :
	'unresolved' RULE_ID+ RULE_ID ';'
;

RULE_ID :
	'^'? (
		'a' .. 'z' |
		'A' .. 'Z' |
		'_'
	) (
		'a' .. 'z' |
		'A' .. 'Z' |
		'_' |
		'0' .. '9'
	)*
;

RULE_INT :
	'0' .. '9'+
;

RULE_STRING :
	'"' (
		'\\' . |
		~ (
			'\\' |
			'"'
		)
	)* '"' |
	'\'' (
		'\\' . |
		~ (
			'\\' |
			'\''
		)
	)* '\''
;

RULE_ML_COMMENT :
	'/*' (
		options { greedy = false ; } : .
	)* '*/' { skip(); }
;

RULE_SL_COMMENT :
	'//' ~ (
		'\n' |
		'\r'
	)* (
		'\r'? '\n'
	)? { skip(); }
;

RULE_WS :
	(
		' ' |
		'\t' |
		'\r' |
		'\n'
	)+ { skip(); }
;

RULE_ANY_OTHER :
	.
;