/*
 * generated by Xtext
 */
grammar DebugInternalUnicodeTestLanguage ;

// Rule Model
ruleModel :
	ruleAbstractString*
;

// Rule AbstractString
ruleAbstractString :
	ruleGString |
	ruleQuotedString
;

// Rule GString
ruleGString :
	RULE_GERMAN_STRING
;

// Rule QuotedString
ruleQuotedString :
	RULE_STRING
;

RULE_GERMAN_STRING :
	(
		'A' .. 'Z' |
		'a' .. 'z' |
		'\u00C4' |
		'\u00D6' |
		'\u00DC' |
		'\u00E4' |
		'\u00F6' |
		'\u00FC' |
		'\u1E9E'
	)*
;

RULE_ID :
	'^'? (
		'a' .. 'z' |
		'A' .. 'Z' |
		'_'
	) (
		'a' .. 'z' |
		'A' .. 'Z' |
		'_' |
		'0' .. '9'
	)*
;

RULE_INT :
	'0' .. '9'+
;

RULE_STRING :
	'"' (
		'\\' . |
		~ (
			'\\' |
			'"'
		)
	)* '"' |
	'\'' (
		'\\' . |
		~ (
			'\\' |
			'\''
		)
	)* '\''
;

RULE_ML_COMMENT :
	'/*' (
		options { greedy = false ; } : .
	)* '*/' { skip(); }
;

RULE_SL_COMMENT :
	'//' ~ (
		'\n' |
		'\r'
	)* (
		'\r'? '\n'
	)? { skip(); }
;

RULE_WS :
	(
		' ' |
		'\t' |
		'\r' |
		'\n'
	)+ { skip(); }
;

RULE_ANY_OTHER :
	.
;