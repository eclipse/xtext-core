/*
 * generated by Xtext
 */
grammar DebugInternalLookaheadTestLanguage ;

// Rule Entry
ruleEntry :
	ruleAlts*
;

// Rule Alts
ruleAlts :
	ruleLookAhead0 |
	ruleLookAhead1 |
	ruleLookAhead3
;

// Rule LookAhead0
ruleLookAhead0 :
	'bar' 'a'
;

// Rule LookAhead1
ruleLookAhead1 :
	'foo' ruleLookAhead2 'b' 'd'
;

// Rule LookAhead2
ruleLookAhead2 :
	(
		'foo' |
		'bar'
	) 'c'
;

// Rule LookAhead3
ruleLookAhead3 :
	'foo' 'bar' 'b' ruleLookAhead4
;

// Rule LookAhead4
ruleLookAhead4 :
	'c' |
	'd'
;

RULE_ID :
	'^'? (
		'a' .. 'z' |
		'A' .. 'Z' |
		'_'
	) (
		'a' .. 'z' |
		'A' .. 'Z' |
		'_' |
		'0' .. '9'
	)*
;

RULE_INT :
	'0' .. '9'+
;

RULE_STRING :
	'"' (
		'\\' . |
		~ (
			'\\' |
			'"'
		)
	)* '"' |
	'\'' (
		'\\' . |
		~ (
			'\\' |
			'\''
		)
	)* '\''
;

RULE_ML_COMMENT :
	'/*' (
		options { greedy = false ; } : .
	)* '*/' { skip(); }
;

RULE_SL_COMMENT :
	'//' ~ (
		'\n' |
		'\r'
	)* (
		'\r'? '\n'
	)? { skip(); }
;

RULE_WS :
	(
		' ' |
		'\t' |
		'\r' |
		'\n'
	)+ { skip(); }
;

RULE_ANY_OTHER :
	.
;