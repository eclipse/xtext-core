/*
 * generated by Xtext
 */
grammar DebugInternalSimpleExpressionsTestLanguage ;

// Rule Sequence
ruleSequence :
	ruleAddition ruleAddition*
;

// Rule Addition
ruleAddition :
	ruleMultiplication (
		(
			'+' |
			'-'
		) ruleMultiplication
	)*
;

// Rule Multiplication
ruleMultiplication :
	ruleTerm (
		(
			'*' |
			'/'
		) ruleTerm
	)*
;

// Rule Term
ruleTerm :
	ruleAtom |
	ruleParens
;

// Rule Atom
ruleAtom :
	RULE_ID
;

// Rule Parens
ruleParens :
	'(' ruleAddition ')'
;

RULE_ID :
	'^'? (
		'a' .. 'z' |
		'A' .. 'Z' |
		'_'
	) (
		'a' .. 'z' |
		'A' .. 'Z' |
		'_' |
		'0' .. '9'
	)*
;

RULE_INT :
	'0' .. '9'+
;

RULE_STRING :
	'"' (
		'\\' . |
		~ (
			'\\' |
			'"'
		)
	)* '"' |
	'\'' (
		'\\' . |
		~ (
			'\\' |
			'\''
		)
	)* '\''
;

RULE_ML_COMMENT :
	'/*' (
		options { greedy = false ; } : .
	)* '*/' { skip(); }
;

RULE_SL_COMMENT :
	'//' ~ (
		'\n' |
		'\r'
	)* (
		'\r'? '\n'
	)? { skip(); }
;

RULE_WS :
	(
		' ' |
		'\t' |
		'\r' |
		'\n'
	)+ { skip(); }
;

RULE_ANY_OTHER :
	.
;