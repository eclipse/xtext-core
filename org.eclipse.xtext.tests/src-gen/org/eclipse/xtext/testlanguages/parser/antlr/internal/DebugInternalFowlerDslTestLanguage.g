/*
 * generated by Xtext
 */
grammar DebugInternalFowlerDslTestLanguage ;

// Rule Statemachine
ruleStatemachine :
	'events' ruleEvent* 'end' 'commands' ruleCommand* 'end' ruleState*
;

// Rule Event
ruleEvent :
	'resetting'? RULE_ID RULE_ID
;

// Rule Command
ruleCommand :
	RULE_ID RULE_ID
;

// Rule State
ruleState :
	'state' RULE_ID (
		'actions' '{' RULE_ID+ '}'
	)? ruleTransition* 'end'
;

// Rule Transition
ruleTransition :
	RULE_ID '=>' RULE_ID
;

RULE_ID :
	'^'? (
		'a' .. 'z' |
		'A' .. 'Z' |
		'_'
	) (
		'a' .. 'z' |
		'A' .. 'Z' |
		'_' |
		'0' .. '9'
	)*
;

RULE_INT :
	'0' .. '9'+
;

RULE_STRING :
	'"' (
		'\\' . |
		~ (
			'\\' |
			'"'
		)
	)* '"' |
	'\'' (
		'\\' . |
		~ (
			'\\' |
			'\''
		)
	)* '\''
;

RULE_ML_COMMENT :
	'/*' (
		options { greedy = false ; } : .
	)* '*/' { skip(); }
;

RULE_SL_COMMENT :
	'//' ~ (
		'\n' |
		'\r'
	)* (
		'\r'? '\n'
	)? { skip(); }
;

RULE_WS :
	(
		' ' |
		'\t' |
		'\r' |
		'\n'
	)+ { skip(); }
;

RULE_ANY_OTHER :
	.
;