RULE_CASEINSENSITIVEKEYWORD :
	(
		'k' |
		'K'
	) (
		'e' |
		'E'
	) (
		'y' |
		'Y'
	) (
		'w' |
		'W'
	) (
		'o' |
		'O'
	) (
		'r' |
		'R'
	) (
		'd' |
		'D'
	)
;

RULE_PLURAL :
	'item' |
	'items'
;

RULE_MULTI :
	'a' 'b'? 'c'+ 'd'* (
		'e' |
		'f'
	) (
		(
			'g' |
			'h'
		) |
		'i'+
	) RULE_MULTI2 '0' .. '9' '0' .. '9'+
;

fragment RULE_MULTI2 :
	'j'
;

RULE_ID :
	'^'? (
		'a' .. 'z' |
		'A' .. 'Z' |
		'_'
