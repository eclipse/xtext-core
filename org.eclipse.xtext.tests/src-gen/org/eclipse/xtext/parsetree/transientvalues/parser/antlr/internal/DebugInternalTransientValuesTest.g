/*
 * generated by Xtext
 */
grammar DebugInternalTransientValuesTest ;

// Rule Root
ruleRoot :
	'test' (
		ruleTestRequired |
		ruleTestOptional |
		ruleTestList
	)
;

// Rule TestRequired
ruleTestRequired :
	'required' RULE_INT RULE_INT
;

// Rule TestOptional
ruleTestOptional :
	'optional' RULE_INT? (
		':' RULE_INT
	)?
;

// Rule TestList
ruleTestList :
	'list' RULE_INT*
;

RULE_ID :
	'^'? (
		'a' .. 'z' |
		'A' .. 'Z' |
		'_'
	) (
		'a' .. 'z' |
		'A' .. 'Z' |
		'_' |
		'0' .. '9'
	)*
;

RULE_INT :
	'0' .. '9'+
;

RULE_STRING :
	'"' (
		'\\' . |
		~ (
			'\\' |
			'"'
		)
	)* '"' |
	'\'' (
		'\\' . |
		~ (
			'\\' |
			'\''
		)
	)* '\''
;

RULE_ML_COMMENT :
	'/*' (
		options { greedy = false ; } : .
	)* '*/' { skip(); }
;

RULE_SL_COMMENT :
	'//' ~ (
		'\n' |
		'\r'
	)* (
		'\r'? '\n'
	)? { skip(); }
;

RULE_WS :
	(
		' ' |
		'\t' |
		'\r' |
		'\n'
	)+ { skip(); }
;

RULE_ANY_OTHER :
	.
;